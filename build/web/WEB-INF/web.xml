        <servlet-name>imgTest</servlet-name>
        <url-pattern>/imgTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Uploadservlet</servlet-name>
        <url-pattern>/Uploadservlet</url-pattern>
    </servlet-mapping>
