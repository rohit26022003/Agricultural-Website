    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>packserver.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImgTestServlet</servlet-name>
        <servlet-class>packserver.ImgTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>packserver.UploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>imgTest</servlet-name>
        <servlet-class>packserver.imgTest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Uploadservlet</servlet-name>
        <servlet-class>packserver.Uploadservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PaymentServlet</servlet-name>
        <url-pattern>/PaymentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidateEmail</servlet-name>
        <url-pattern>/ValidateEmail</url-pattern>
    </servlet-mapping>
